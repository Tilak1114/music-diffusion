            "weight_decay": 1e-8,
            "gradient_accumulation_steps": 4,
            "lr_scheduler": "linear",
            "adam_beta1": 0.9,
            "adam_beta2": 0.999,
            "adam_weight_decay": 1e-2,
            "adam_eps": 1e-08,
            "unet": {
                "in_channels": 8,
                "block_out_channels": [
                    320,
                    640,
                    1280,
                    1280
                ],
                "down_block_types": [
                    "CrossAttnDownBlock2DMusic",
                    "CrossAttnDownBlock2DMusic",
                    "CrossAttnDownBlock2DMusic",
                    "DownBlock2D"
                ],
                "attention_head_dim": [
                    4,
                    8,
                    16,
                    16
                ],
                "up_block_types": [
                    "UpBlock2D",
                    "CrossAttnUpBlock2DMusic",
                    "CrossAttnUpBlock2DMusic",
                    "CrossAttnUpBlock2DMusic"
                ]
            }
        },
        "trainer_config": {
            "batch_size":16,
            "max_epochs": 100,
            "devices": -1
        }
    },
